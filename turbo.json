{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": [".dist/**", "out/**", ".build/**", ".next/**"]
        },
        "test": {
            "dependsOn": ["build"],
            "inputs": ["src/**/*.{ts,tsx}"]
        }
    }
}
