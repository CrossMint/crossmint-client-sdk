{
    "name": "@crossmint/client-sdk-base",
    "version": "0.2.7-alpha.17",
    "author": "Paella Labs Inc",
    "license": "Apache-2.0",
    "repository": "https://github.com/CrossMint/crossmint-client-sdk",
    "sideEffects": false,
    "main": "lib/index.js",
    "module": "lib/index.mjs",
    "types": "lib/index.d.ts",
    "exports": {
        ".": {
            "import": "./lib/index.mjs",
            "require": "./lib/index.js",
            "types": "./lib/index.d.ts"
        }
    },
    "files": [
        "lib",
        "src",
        "LICENSE"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "clean": "shx rm -rf lib/*",
        "build": "pnpm run clean && tsup src/index.ts --format esm,cjs --outDir ./lib --minify --dts"
    },
    "dependencies": {
        "uuid": "9.0.0"
    },
    "devDependencies": {
        "@types/uuid": "9.0.1"
    }
}
