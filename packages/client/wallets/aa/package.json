{
    "name": "@crossmint/client-sdk-aa",
    "version": "0.0.13",
    "author": "Paella Labs Inc",
    "license": "Apache-2.0",
    "repository": "https://github.com/Crossmint/crossmint-sdk",
    "type": "module",
    "sideEffects": false,
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "import": "./dist/index.js",
