    },
    "dependencies": {
        "@ambire/signature-validator": "1.3.1",
        "@fireblocks/ncw-js-sdk": "9.5.0",
        "@web3auth/base": "^6.0.1-alpha.0",
        "@web3auth/ethereum-provider": "^6.1.7",
        "@web3auth/single-factor-auth": "^6.5.0",
        "@zerodev/sdk": "4.5.2",
        "ethers": "5.7.2",
        "viem": "1.5.3"
    }
