{
    "entryPoints": ["src/index.ts"],
    "out": "docs",
    "plugin": [
        "typedoc-plugin-markdown",
        "typedoc-plugin-frontmatter",
        //"typedoc-plugin-missing-exports", // this can be used to document way more stuff
        "./custom-plugin.mjs"
    ],
    "tsconfig": "tsconfig.typedoc.json",
    "fileExtension": ".mdx",
    "skipErrorChecking": true,
    "excludeNotDocumented": false,
    "hidePageHeader": true,
    "hideBreadcrumbs": true,
    "hidePageTitle": true,
    "parametersFormat": "table"
    //"placeInternalsInOwningModule": true // for missing exports plugin"
}
