{
    "name": "@crossmint/client-sdk-react-ui",
    "version": "1.3.20",
    "repository": "https://github.com/Crossmint/crossmint-sdk",
    "license": "Apache-2.0",
    "author": "Paella Labs Inc",
    "sideEffects": false,
    "type": "module",
    "exports": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "src",
        "LICENSE"
    ],
    "scripts": {
        "prebuild": "pnpm run create-version-file",
        "build": "tsup src/index.ts --clean --external react,react-dom --format esm,cjs --outDir ./dist --minify --dts --sourcemap && pnpm run styles",
        "postbuild": "pnpm run styles:inject",
        "create-version-file": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/consts/version.ts",
        "dev": "concurrently \"node scripts/watch-and-inject.js\" \"pnpm run styles:watch\"",
        "styles": "npx tailwindcss -i ./src/tailwind.css -o ./dist/styles/output.css",
        "styles:inject": "node scripts/inject-css-import.js",
        "styles:watch": "pnpm run styles --watch",
        "test": "vitest run",
        "version": "pnpm run create-version-file && git add ."
    },
    "dependencies": {
        "@crossmint/client-sdk-auth-core": "workspace:*",
        "@crossmint/client-sdk-base": "workspace:*",
        "@crossmint/client-sdk-smart-wallet": "workspace:*",
        "@crossmint/client-sdk-window": "workspace:*",
        "@crossmint/common-sdk-base": "workspace:*",
        "@ethersproject/transactions": "5.7.0",
        "@headlessui/react": "2.1.5",
        "@solana/web3.js": "1.95.1",
        "bs58": "5.0.0",
        "lodash.isequal": "4.5.0",
        "react-jss": "10.10.0",
        "zod": "3.22.4"
    },
    "devDependencies": {
        "@types/lodash.isequal": "4.5.6",
        "@types/react": "^18.3.0",
        "@types/react-dom": "^18.3.0",
        "chokidar": "4.0.0",
        "concurrently": "9.0.1",
        "tailwindcss": "3.4.11"
    },
    "peerDependencies": {
        "react": "^18.3.0",
        "react-dom": "^18.3.0"
    }
}
