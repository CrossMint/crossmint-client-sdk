{
    "name": "@crossmint/client-sdk-react-ui",
    "version": "1.0.1-alpha.6",
    "author": "Paella Labs Inc",
    "license": "Apache-2.0",
    "repository": "https://github.com/Crossmint/crossmint-client-sdk",
    "type": "module",
    "sideEffects": false,
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
    },
    "files": [
        "dist",
        "src",
        "LICENSE"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "create-version-file": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/consts/version.ts",
        "version": "yarn run create-version-file && git add .",
        "prebuild": "yarn run create-version-file",
        "clean": "shx rm -rf dist/*",
        "build": "yarn clean && tsup src/index.ts --external react,react-dom --format esm,cjs --outDir ./dist --minify --dts",
        "test": "jest"
    },
    "dependencies": {
        "@crossmint/client-sdk-base": "^1.0.1-alpha.6",
        "react-jss": "10.9.2",
        "uuid": "^8.3.2"
    },
    "peerDependencies": {
        "react": ">=17.0.2",
        "react-dom": ">=17.0.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@types/jest": "^27.4.1",
        "@types/react": "17.0.34",
        "@types/react-dom": "^17.0.11",
        "jest": "^27.5.1",
        "node-fetch": "3.2.10",
        "ts-jest": "^27.1.3",
        "ts-node-dev": "^1.1.8"
    }
}
