{
    "name": "@crossmint/client-sdk-react-ui",
    "version": "0.0.8-alpha.4",
    "author": "Paella Labs Inc",
    "license": "Apache-2.0",
    "repository": "https://github.com/CrossMint/crossmint-client-sdk",
    "type": "module",
    "sideEffects": false,
    "main": "lib/cjs/index.js",
    "module": "lib/esm/index.js",
    "types": "lib/esm/index.d.ts",
    "exports": {
        ".": {
            "import": "./lib/esm/index.js",
            "require": "./lib/cjs/index.js"
        }
    },
    "files": [
        "lib",
        "src",
        "LICENSE"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "create-version-file": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
        "version": "yarn run create-version-file && git add .",
        "prebuild": "yarn run create-version-file",
        "clean": "shx rm -rf lib/*",
        "build": "yarn clean && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
        "postbuild": "shx echo '{ \"type\": \"commonjs\" }' > lib/cjs/package.json && shx echo '{ \"type\": \"module\" }' > lib/esm/package.json"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@types/react": "^17.0.24",
        "@types/react-dom": "^17.0.11",
        "@types/styled-components": "^5.1.23",
        "babel-plugin-styled-components": "^2.0.5",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "styled-components": "^5.3.3",
        "uuid": "^8.3.2"
    }
}
