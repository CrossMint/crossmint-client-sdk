{
    "name": "@crossmint/client-sdk-react-ui",
    "version": "0.2.7-alpha.17",
    "author": "Paella Labs Inc",
    "license": "Apache-2.0",
    "repository": "https://github.com/CrossMint/crossmint-client-sdk",
    "sideEffects": false,
    "main": "lib/index.js",
    "module": "lib/index.mjs",
    "types": "lib/index.d.ts",
    "exports": {
        ".": {
            "import": "./lib/index.mjs",
            "require": "./lib/index.js",
            "types": "./lib/index.d.ts"
        }
    },
    "files": [
        "lib",
        "src",
        "LICENSE"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "create-version-file": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
        "version": "pnpm run create-version-file && git add .",
        "prebuild": "pnpm run create-version-file",
        "clean": "shx rm -rf lib/*",
        "build": "pnpm run clean && tsup src/index.ts --external react,react-dom --format esm,cjs --outDir ./lib --minify --dts",
        "test": "jest"
    },
    "dependencies": {
        "@crossmint/client-sdk-base": "workspace:*",
        "jss": "10.10.0",
        "react-jss": "10.10.0",
        "uuid": "9.0.0"
    },
    "peerDependencies": {
        "react": ">=17.0.2",
        "react-dom": ">=17.0.2"
    },
    "devDependencies": {
        "@jest/globals": "^29.5.0",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "14.0.0",
        "@types/jest": "27.5.2",
        "@types/react": "17.0.55",
        "@types/react-dom": "17.0.19",
        "node-fetch": "3.3.1"
    }
}
